<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/poema1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poema # 3"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="317dp"
        android:onClick="OnClick1"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adios!"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="712dp" />

    <TextView
        android:id="@+id/txtguatemala"
        android:layout_width="195dp"
        android:layout_height="35dp"
        android:text="Bienvenidos a Guatemala"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="45dp" />

    <Button
        android:id="@+id/Poema1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:text="Poema # 1"
        app:layout_constraintBottom_toTopOf="@+id/Poema2"
        tools:layout_editor_absoluteX="141dp"
        android:onClick="OnClick2"/>

    <Button
        android:id="@+id/Poema2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poema # 2"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="225dp"
        android:onClick="OnClick3"/>

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/imageFilterView"
        android:layout_width="281dp"
        android:layout_height="219dp"
        app:srcCompat="@drawable/quetzal"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="381dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>